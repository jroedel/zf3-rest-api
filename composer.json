{
	"name" : "multidots/zf3-rest-api",
	"description" : "Zend Framework 3 module to build REST APIs",
	"type" : "library",
	"license" : "MIT",
	"homepage" : "https://github.com/multidots/zf3-rest-api",
	"authors" : [{
			"name" : "Multidots",
			"homepage" : "http://www.multidots.com",
			"role" : "Author"
		}, {
			"name" : "Community",
			"homepage" : "https://github.com/multidots/zf3-rest-api/graphs/contributors"
		}
	],
	"support" : {
		"issues" : "https://github.com/multidots/zf3-rest-api/issues",
		"source" : "https://github.com/multidots/zf3-rest-api"
	},
	"keywords" : [
		"zf3",
		"zend framework",
		"cors",
		"rest",
		"rest api"
	],
	"require" : {
		"firebase/php-jwt" : "^4.0",
		"zendframework/zend-json" : "^3.0"
	},
	"suggest" : {
		"zfr/zfr-cors" : "Needed to respond handle CORS headers enabling APIs to be called from cross-origin javascript"
	},
	"autoload" : {
		"psr-4" : {
			"RestApi\\" : "src/"
		}
	},
	"extra" : {
		"zf" : {
			"module" : "RestApi"
		}
	}
}